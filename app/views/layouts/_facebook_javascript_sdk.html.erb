<div id="fb-root"></div>
<script>
  window.fbAsyncInit = function() {
    FB.init({appId: '<%= Sutalk::Application.config.facebook_api[:app_id] %>', status: true, cookie: true,
             xfbml: true});

		<%= facebookJavascriptOnload %>
			 
  };
  (function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());

	var fb_api = eval( <%= Sutalk::Application.config.facebook_api[:json] %> );
</script>


